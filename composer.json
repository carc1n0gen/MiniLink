{
    "name": "carc1n0gen/weelnk",
    "description": "Small self hostable link shortening service written in PHP with the slim micro framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Carson Evans",
            "email": "carsonwevans@gmail.com"
        }
    ],
    "require": {
        "carc1n0gen/short-link": "^1.1",
        "davedevelopment/phpmig": "dev-master#e24b303936931c9b912f13ad0a3ea8351efa8f00",
        "illuminate/cache": "^5.4",
        "illuminate/database": "^5.4",
        "illuminate/filesystem": "^5.4",
        "illuminate/redis": "^5.4",
        "monolog/monolog": "^1.22",
        "slim/php-view": "^2.2",
        "slim/slim": "^3.7",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
